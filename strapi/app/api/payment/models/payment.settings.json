{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "name": "Payment",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "currency": {
      "type": "enumeration",
      "enum": [
        "AUD",
        "USD"
      ]
    },
    "amount": {
      "type": "float"
    },
    "datePaid": {
      "type": "datetime"
    },
    "participant": {
      "via": "payments",
      "model": "participants"
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "transmitted",
        "confirmed"
      ]
    },
    "paymentFilesLink": {
      "type": "string"
    }
  }
}
